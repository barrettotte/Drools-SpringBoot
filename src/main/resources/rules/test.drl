package rules

import com.barrettotte.premium.model.Vehicle;

import java.math.BigDecimal;


rule "Test Premium 1"
	dialect "mvel"
    when
        $vehicle : Vehicle(
			"Jeep".equalsIgnoreCase(make),
			"Cherokee".equalsIgnoreCase(model),
			1997 == year
		)
    then
		$vehicle.setPremium(BigDecimal.valueOf(25.0));
end

